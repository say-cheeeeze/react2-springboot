<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cheeeeze.reactstd.v1.web.user.service.UserRepository">

    <sql id="select">
        id          id,
        name          name,
        inputDate inputDate,
        updateDate updateDate
    </sql>

    <select id="listAll">
        select
        <include refid="select"/>
        from userTBL
    </select>

</mapper>
